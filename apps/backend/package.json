    "#database/*": [
      "./dist/database/*"
    ],
    "#utils/*": [
      "./dist/utils/*"
    ],
    "#decorators/*": [
      "./dist/decorators/*"
    ],
    "#guards/*": [
      "./dist/guards/*"
    ],
    "#modules/*": [
      "./dist/modules/*"
    ],
    "#config/*": [
      "./dist/config/*"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
